   <select id="getExemptions" resultType="java.util.Map" parameterType="java.util.Map" >
  select coalesce(e.exemptions, 0) as exemptions, coalesce(e.hoursRequired, e2.hoursRequired) as hoursRequired
from person p
        left join exemptions ex on p.id = ex.personId and endTerm = #{endterm,jdbcType=DATE}
        left join exempt e on ex.exempt = e.exemptions and e.createDate = (
                select max(createDate)
                from exempt
                )
        join exempt e2 on e2.exemptions = 0 and e2.createDate = (
                select max(createDate)
                from exempt)
where currentStudent = 1
        and people_id = #{peopleId,jdbcType=VARCHAR}
    </select>
