    <select id="selectPresent" resultType="java.util.Map" parameterType="java.lang.String" >
--present query
select cast(se.starttime as date) as day, min(case when tardy = 0 then value when se.allcredit = 1 then e.attendanceValue else 0 end) as value
from scheduledEvent se
  join event e on se.event = e.id
  left join punch p on p.campusId = #{campusId,jdbcType=VARCHAR} and se.id = eventschedule
where case when tardy = 0 then value when se.allcredit = 1 then e.attendanceValue else 0 end &lt;&gt; 0  
  and se.starttime &gt;= #{startterm,jdbcType=TIMESTAMP}
group by campusId, se.event, cast(se.starttime as date)
order by cast(se.starttime as date) desc
  </select>
  
<select id="selectPresentCount" resultType="java.util.Map" parameterType="java.lang.String" >
select count(day) as count, coalesce(sum(value), 0) as total
from (
  select cast(se.starttime as date) as day, min(case when tardy = 0 then value when se.allcredit = 1 then e.attendanceValue else 0 end) as value
from scheduledEvent se
  join event e on se.event = e.id
  left join punch p on p.campusId = #{campusId,jdbcType=VARCHAR} and se.id = eventschedule
where case when tardy = 0 then value when se.allcredit = 1 then e.attendanceValue else 0 end &lt;&gt; 0  
  and se.starttime &gt;= #{startterm,jdbcType=TIMESTAMP}
group by campusId, se.event, cast(se.starttime as date)
) att
  </select>
  
    <select id="selectTardy" resultType="java.util.Map" parameterType="java.lang.String" >
--tardy query
select cast(se.starttime as date) as day, min(value) as value
from scheduledEvent se
  join event e on se.event = e.id
  left join punch p on p.campusId = #{campusId,jdbcType=VARCHAR} and se.id = eventschedule
where tardy = 1
  and se.starttime  &gt;= #{startterm,jdbcType=TIMESTAMP}
group by campusId, se.event, cast(se.starttime as date)
order by cast(se.starttime as date) desc
  </select>
  
